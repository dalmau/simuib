\end_layout

\begin_layout Standard

        SQRTRT3 += pow(rTSum3/served3, 2);
\end_layout

\begin_layout Standard

        SQRTRTS += pow((rTSum1+rTSum2+rTSum3)/served, 2);
\end_layout

\begin_layout Standard

}
